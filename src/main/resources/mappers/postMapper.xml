<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.walk.post.mapper.PostMapper">
 
 	<select id="selectPostListTest" resultType="map">
 		SELECT
 			*
 		FROM
 			`post` 	
 	</select>
 	
 	 <insert id="insertPost" parameterType="map">
 		INSERT INTO `post`
 		(
 			`userId`
 			, `subject`
 			, `place`
 			, `distance`
 			, `time`
 			, `Date`
 			, `content`
 			, `imagePath`
 			, `createdAt`
			, `updatedAt`
 		)
 		VALUES
 		(
 			#{userId}
 			, #{subject}
 			, #{place}
 			, #{distance}
 			, #{time}
 			, #{visitDate}
 			, #{placeExplain}
 			, #{imagePath}
 			, NOW()
 			, NOW()
 		)
 	</insert>
 
 </mapper>